{#
This is a template fragment designed to be included in other templates.
It renders all flash messages using the '_single_alert_message.html.twig' template.
#}
{% for type, messages in app.session.flashBag.all %}
    {% for message in messages %}
        {% include 'default/_single_alert_message.html.twig' with { type: type, message: message } %}
    {% endfor %}
{% endfor %}
